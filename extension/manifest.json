{
  "manifest_version": 3,
  "name": "PhishSleuth",
  "version": "1.0",
  "description": "Detect phishing by analyzing page content in real time.",
  "permissions": ["scripting", "tabs", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

  